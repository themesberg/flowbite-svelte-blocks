{
  "name": "flowbite-svelte-blocks",
  "version": "0.3.4",
  "description": "Flowbite blocks components for Svelte",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/shinokada/flowbite-svelte-blocks/issues",
  "homepage": "https://github.com/shinokada/flowbite-svelte-blocks",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "test": "playwright test",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "write-package-json": "node ./scripts/package-json-writer.js",
    "package:publish": "standard-version && git push --follow-tags origin main && npm run package && npm run write-package-json && npm publish"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.6",
    "classnames": "^2.3.2",
    "flowbite": "^1.6.3",
    "flowbite-svelte": "^0.30.0",
    "svelte": "^3.54.0",
    "svelte-heros": "^2.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@popperjs/core": "^2.11.6",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.12.0",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "flowbite": "^1.6.4",
    "flowbite-svelte": "^0.34.1",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "publint": "^0.1.10",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-heros": "^3.1.0",
    "svelte-meta-tags": "^2.7.0",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "flowbite",
    "flowbite blocks"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/flowbite-svelte-blocks"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
